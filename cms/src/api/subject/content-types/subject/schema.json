{
  "kind": "collectionType",
  "collectionName": "subjects",
  "info": {
    "singularName": "subject",
    "pluralName": "subjects",
    "displayName": "Subject",
    "description": "Subjects within exam courses"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "code": {
      "type": "uid",
      "targetField": "name",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "examCourses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::exam-course.exam-course",
      "inversedBy": "subjects"
    },
    "units": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::unit.unit",
      "mappedBy": "subjects"
    }
  }
}
