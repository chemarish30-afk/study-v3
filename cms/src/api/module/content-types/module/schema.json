{
  "kind": "collectionType",
  "collectionName": "modules",
  "info": {
    "singularName": "module",
    "pluralName": "modules",
    "displayName": "Module",
    "description": "Modules within chapters"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "code": {
      "type": "uid",
      "targetField": "title",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true,
      "unique": true
    },
    "type": {
      "type": "enumeration",
      "enum": ["lecture", "practice", "assessment", "reference"],
      "required": true
    },
    "chapters": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::chapter.chapter",
      "inversedBy": "modules"
    },
    "contents": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::content.content",
      "mappedBy": "modules"
    }
  }
}
